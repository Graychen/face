page {
    background: #fff url(data:image/png;base64,) no-repeat 0 0;
    background-size: 750rpx auto;
}

page .sign {
    width: 100vw;
    margin-top: 150rpx;
}

page .sign .content {
    width: 90vw;
    z-index: 99;
}

page .sign .content .img {
    width: 50rpx;
    height: 50rpx;
}

page .sign .content .signed .money {
    color: #fff;
    background-image: linear-gradient(135deg,#ffb90d,#fdce00);
}

page .sign .content .signed .color {
    color: #b2b2b2;
}

page .sign .content .not-signed .money {
    color: #aea9af;
    background-image: linear-gradient(135deg,#f5f1f2,#f7f7f7);
}

page .sign .content .not-signed .color {
    color: #686868;
}

page .sign .bg {
    width: 100vw;
    height: 50%;
    position: absolute;
    top: 50%;
    right: 0;
    bottom: 0;
    left: 0;
    background-color: #fff;
}

page .task {
    width: 90vw;
}

page .task .line {
    width: 6rpx;
    height: 30rpx;
    background-color: #fbbd08;
}

page .task .project-desc {
    margin-left: 26rpx;
}